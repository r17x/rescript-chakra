{
  "name": "website-rescript-chakra",
  "version": "0.0.0",
  "license": "MIT",
  "description": "The Documentation of rescript-chakra",
  "dependencies": {
    "@chakra-ui/react": "^1.6.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^4",
    "next": "10.2.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rescript-chakra": "latest"
  },
  "repository": "https://github.com/ri7nz/rescript-chakra-nextjs-template",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:compiler": "rescript build -w",
    "dev:next": "wait-on src/App.mjs && next",
    "debug": "NODE_OPTIONS='--inspect' next",
    "build": "rescript && next build",
    "now-build": "rescript && next build",
    "export": "next export",
    "start": "next start -p $PORT",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:start": "rescript build -w"
  },
  "devDependencies": {
    "@rescript/react": "0.10.3",
    "gentype": "4.1",
    "next-transpile-modules": "7.1.2",
    "npm-run-all": "4.1.5",
    "rescript": "9.1",
    "wait-on": "6.0.0"
  }
}
